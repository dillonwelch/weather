<%= form_with url: "/search" do |form| %>
  <%= form.label :street %>
  <%= form.text_field :street, { value: params[:street] } %>
  </br>
  <%= form.label :city %>
  <%= form.text_field :city, { value: params[:city] } %>
  </br>
  <%= form.label :state %>
  <%= form.text_field :state, { value: params[:state] } %>
  </br>
  <%= form.label :zip %>
  <%= form.text_field :zip, { value: params[:zip] } %>
  </br>
  <%= form.submit "Search" %>
<% end %>

